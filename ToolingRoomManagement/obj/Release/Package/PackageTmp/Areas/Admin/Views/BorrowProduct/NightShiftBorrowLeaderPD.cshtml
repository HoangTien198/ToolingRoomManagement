<script>
    var modal = document.getElementById("ModalCreateOrder");
    var modelReject = document.getElementById("myModalReject");
    function closeModal() {
        modal.style.display = "none";
    }
    function closeRejectModel() {
        modelReject.style.display = "none";
    }
    function rejectModel() {
        var id_order = $("#order_id").val();
        $("#id_order").val(id_order);
        modelReject.style.display = "block";
    }
    function ModalCreate() {
        $("#code_order").empty();
        $("#order_id").val("");
        $("#managerUs").val("");
        $("#dateBorrow").val("");
        $("#noteOrder").val("");
        $("#managerUs").val(0);
        $("#productName").val("");
        $("#productQuantity").val(1);
        $("#productLine").val(0);
        $("#leaderLineCode").val("");
        $("#leaderLineName").val("");
        $("#tbodyView").empty();

        modal.style.display = "block";
    }
    function closeModal() {
        modal.style.display = "none";
    }
    function reloadPage() {
        var number = $("#select_status").val();
        loadData(number);
        modal.style.display = "none";
    }
    $(document).ready(function () {
        $("#userID").hide();
        $("#load_section").show();
        $("#tblView").show();
        loadData(100);
    });
    $("#select_status").change(function () {
        $(".load_section").show();
        $("#tbd_View").hide();
        var number = $("#select_status").val();
        loadData(number)
    })
    function loadData(number) {
        $.ajax({
            type: "Post",
            url: "/GetData/lisOderForecastPD?number=" + number,
            async: false,
            contentType: false,
            processData: false,
            success: function (data) {
                var row = "";
                $("#tbd_View").empty();
                if (JSON.parse(data).length > 0) {

                    for (i = 0; i < JSON.parse(data).length; i++) {
                        var status = "";
                        if (JSON.parse(data)[i].status == 12) {
                            status = '<a style="color:orange">Đợi xác nhận</a>';
                        } else if (JSON.parse(data)[i].status == 13) {
                            status = '<a style="color:green">Đã xác nhận</a>';
                        } else if (JSON.parse(data)[i].status == 14) {
                            status = '<a style="color:red">Từ chối</a>';
                        } else if (JSON.parse(data)[i].status == 15) {
                            status = '<a style="color:green">Đã bù</a>';
                        }
                        row += '<tr>' +
                            '<td>' + (i + 1) + '</td>' +
                            '<td><a onclick="detailOrder(' + "'" + JSON.parse(data)[i].id + "','" + JSON.parse(data)[i].codeOrder + "','" + JSON.parse(data)[i].status + "','" + convert_date2(JSON.parse(data)[i].date_borrow) + "','" + (JSON.parse(data)[i].note != null ? StringToHex( JSON.parse(data)[i].note.replace("\n", " ").replace("\r", " ")) : "") + "','" + JSON.parse(data)[i].managerUs_id + "','" + JSON.parse(data)[i].create_by + "','" + JSON.parse(data)[i].create_by_id + "'" + ')">' + JSON.parse(data)[i].codeOrder + '</td>' +
                            '<td>' + status + '</td>' +
                            '<td>' + convert_date2(JSON.parse(data)[i].date_borrow) + '</td>' +
                            '<td>' + JSON.parse(data)[i].create_by + '</td>' +
                            '<td>' + convert_date2(JSON.parse(data)[i].create_date) + '</td>' +
                            '<td>' + (JSON.parse(data)[i].note != null ? JSON.parse(data)[i].note : "") + '</td>' +
                             '<td>' + JSON.parse(data)[i].accountantPD_username + '</td>' +
                            '<td>' + JSON.parse(data)[i].aTin_username + '</td>' +
                            '<td style="color: chartreuse;">' + (JSON.parse(data)[i].time_aTin_cf != null ? convert_date2(JSON.parse(data)[i].time_aTin_cf) : "") + '</td>' +
                            '<td style="color: #ff0018;">' + (JSON.parse(data)[i].note_aTin != null ? JSON.parse(data)[i].note_aTin : "") + '</td>' +
                            '</tr>';
                    }
                } else {
                    row = '<tr><td colspan="24" style="text-align:center">-------------K có đơn-----------</td></tr>';
                }
                $(".load_section").hide();
                $("#tbd_View").show();
                $("#tbd_View").append(row);
            },
            error: function (err) {
                //debugger
                alert("FAIL!");
            }
        });
    }
    function detailOrder(id, code_order, status, date_borrow, note_order, managerUs_id, create_by, create_by_id) {
        $("#code_order").empty();
        $("#order_id").val("");
        $("#userID").val("");
        $("#employeeID").empty();
        $("#fullname").empty();
        $("#dateBorrow").val("");
        $("#noteOrder").val("");
        $("#productName").val("");
        $("#productQuantity").val(1);
        $("#productLine").val(0);
        $("#leaderLineCode").val("");
        $("#leaderLineName").val("");

        $("#order_id").val(id);
        $("#code_order").append(code_order);
        var username = create_by.split('-')[0];
        var fullname = create_by.split('-')[1];
        $("#employeeID").val(username);
        $("#userID").val(create_by_id);
        $("#fullname").val(fullname);
        var dateBorrow_ = date_borrow.split('-')[2] + '-' + date_borrow.split('-')[1] + '-' + date_borrow.split('-')[0];
        $("#dateBorrow").val(dateBorrow_);
        $("#noteOrder").val(HexToString( note_order));
