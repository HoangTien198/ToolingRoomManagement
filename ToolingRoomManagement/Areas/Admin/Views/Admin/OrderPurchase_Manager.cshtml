<div class="product-sales-area mg-tb-30" style="">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"  style="height: 740px;">
                <div class="product-sales-chart"  style="height: 710px;">
                    <div class="portlet-title">
                        <div class="row">
                            <div class="content">
                                <div class="" style="margin-left:190px;width:100%;overflow-y:auto;">
                                    @if (Model.Count() != 0)
                                    {
                                        foreach (var item in Model)
                                        {
                                            { dem++; }
                                            <div class="zoom" style="margin-bottom: 10px;font-size: 1.3rem;border-radius: 13px;border: 1px solid #f39800;background: #152036;width: 75%;padding: 40px 15px 75px 15px;color: #fff;cursor: pointer;font-weight: 600;">

                                                <div style="width:80%;float:left;">
                                                    <i class="fa fa-hand-pointer-o" aria-hidden="true" style="color: aquamarine;"></i> <a href="/Admin/Admin/OrderPurchase_Detail_Manager?id=@item.id" style="font-size: 1.3rem;color:#41ead4">Đơn @dem: @item.item_name</a>
                                                    <a>(@item.create_date.Value.ToString("dd/MM/yyyy"))</a>
                                                </div>
                                                @*<button style="border: 0;background: #152036;float: right; margin-right: 20px;" onclick="DeleteReceive(@item.id)"><i class="fa fa-trash" style="color:red;"></i></button>*@
                                            </div>

                                        }
                                    }
                                    else
                                    {
                                        <h4 style="color:#fff"> Dont have any order!</h4>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>
    </div>
</div>

