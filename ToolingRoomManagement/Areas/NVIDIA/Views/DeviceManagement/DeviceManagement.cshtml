@{
    ViewBag.Title = "Device Management";
    Layout = "~/Areas/NVIDIA/Views/Shared/_NvidiaLayoutShared.cshtml";
}

@section styles{
    <!-- Vendor -->
    <link href="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/datatable/css/dataTables.bootstrap5.min.css" rel="stylesheet" />
    <link href="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/select2/css/select2-bootstrap4.css" rel="stylesheet" />
    <link href="~/Areas/NVIDIA/Content/Vendor/Viewer JS/viewer.min.css" rel="stylesheet" />
    <!-- Custom -->
    <link href="~/Areas/NVIDIA/Content/Custom/DeviceManagement/css/DeviceManagement.css?v=@Guid.NewGuid()" rel="stylesheet" />
}
<!-- Breadcrumb -->
<div class="page-breadcrumb d-none d-sm-flex align-items-center py-3">
    <div class="breadcrumb-title pe-3" data-i18n="device.management.management">Management</div>
    <div class="ps-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a href="/NVIDIA/Dashboard"><i class="bx bx-home-alt"></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page" data-i18n="device.management.device">Device</li>
            </ol>
        </nav>
    </div>
</div>

<!-- Card table -->
<div class="card" id="card-device-details">
    <div class="card-body">
        <div class="card-title d-flex justify-content-between align-items-center">
            <div class="d-flex">
                <h5 class="m-0 me-3" data-i18n="device.management.device_details">Device Details</h5>
                <label class="text-secondary" data-i18n="device.management.table_hint">(* Double click to view details)</label>
            </div>

            <div class="w-50 d-flex justify-content-end">
                <div class="input-group input-group-sm w-50">
                    <span class="col-5 input-group-text" data-i18n="device.management.warehouse">Warehouse</span>
                    <select class="col-7 form-select form-select-sm" id="input_WareHouse"></select>
                </div>

                <div class="dropdown ms-3" id="dropdown_filter">
                    <button class="btn btn-sm btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="bx bx-filter"></i></button>
                    <div class="dropdown-menu" style="width:600px;">
                        <div class="row">
                            <div class="col-6">
                                <!--Filter product -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.product">Product</span>
                                    <select class="form-select form-select-sm" id="filter_Product">
                                        <option value="" selected data-i18n="device.management.product_all">Product (All)</option>
                                    </select>
                                </div>
                                <!--Filter model -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.model">Model</span>
                                    <select class="form-select form-select-sm" id="filter_Model">
                                        <option value="" selected data-i18n="device.management.model_all">Model (All)</option>
                                    </select>
                                </div>
                                <!--Filter station -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.station">Station</span>
                                    <select class="form-select form-select-sm" id="filter_Station">
                                        <option value="" selected data-i18n="device.management.station_all">Station (All)</option>
                                    </select>
                                </div>
                                <!--Filter group -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.group">Group</span>
                                    <select class="form-select form-select-sm" id="filter_Group">
                                        <option value="" selected data-i18n="device.management.group_all">Group (All)</option>
                                    </select>
                                </div>

                            </div>
                            <div class="col-6">
                                <!--Filter Vendor -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.vendor">Vendor</span>
                                    <select class="form-select form-select-sm" id="filter_Vendor">
                                        <option value="" selected data-i18n="device.management.vendor_all">Vendor (All)</option>
                                    </select>
                                </div>
                                <!--Filter Type -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.type">Type</span>
                                    <select class="form-select form-select-sm" id="filter_Type">
                                        <option value="Type" selected data-i18n="device.management.type_all">Type (All)</option>
                                        <option value="N/A">N/A </option>
                                        <option value="Static">Static</option>
                                        <option value="Dynamic">Dynamic</option>
                                        <option value="Consign">Consign</option>
                                        <option value="Fixture">Fixture</option>
                                    </select>
                                </div>
                                <!--Filter Status -->
                                <div class="input-group input-group-sm mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.status">Status</span>
                                    <select class="form-select form-select-sm" id="filter_Status">
                                        <option value="Status" selected data-i18n="device.management.status_all">Status (All)</option>
                                        <option value="Unconfirmed">Unconfirmed</option>
                                        <option value="Part Confirmed">Part Confirmed</option>
                                        <option value="Confirmed">Confirmed</option>
                                        <option value="Locked">Locked</option>
                                        <option value="Out Range">Out Range</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-12 d-flex justify-content-between">
                                <button class="btn btn-sm btn-primary" style="width: 80%" id="filter" data-i18n="device.management.filter"><i class="fa-solid fa-arrow-down-small-big"></i> Filter</button>
                                <button class="btn btn-sm btn-outline-secondary ms-3" id="filter-refresh" data-i18n="device.management.refresh"><i class="fa-sharp fa-regular fa-arrows-rotate"></i> Refresh</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <hr />
        <div class="form-body mt-4">
            <table id="table_Devices" class="table table-striped table-hover table-bordered text-nowrap">
                <thead>
                    <tr>
                        <!-- 0 -->
                        <th>ID</th>
                        <!-- 0 -->
                        <th>MTS</th>
                        <!-- 1 -->
                        <th data-i18n="device.management.product_name">Product Name</th>
                        <!-- 2 -->
                        <th data-i18n="device.management.model">Model</th>
                        <!-- 3 -->
                        <th data-i18n="device.management.station">Station</th>
                        <!-- 4 -->
                        <th data-i18n="device.management.pn">PN</th>
                        <!-- 5 -->
                        <th data-i18n="device.management.description">Description</th>
                        <!-- 6 -->
                        <th data-i18n="device.management.group">Group</th>
                        <!-- 7 -->
                        <th data-i18n="device.management.vendor">Vendor</th>
                        <!-- 8 -->
                        <th data-i18n="device.management.specification">Specification</th>
                        <!-- 9 -->
                        <th data-i18n="device.management.location">Location</th>
                        <!-- 10 -->
                        <th data-i18n="device.management.buffer">Buffer</th>
                        <!-- 11 -->
                        <th data-i18n="device.management.bom_qty">BOM Qty</th>
                        <!-- 12 -->
                        <th data-i18n="device.management.po_qty">PO Qty</th>
                        <!-- 13 -->
                        <th data-i18n="device.management.confirm_qty">Confirm Qty</th>
                        <!-- 14 -->
                        <th data-i18n="device.management.real_qty">Real Qty</th>
                        <!-- 15 -->
                        <th data-i18n="device.management.unit">Unit</th>
                        <!-- 16 -->
                        <th data-i18n="device.management.lead_time">Lead Time</th>
                        <!-- 17 -->
                        <th data-i18n="device.management.type"> Type</th>
                        <!-- 18 -->
                        <th data-i18n="device.management.status">Status</th>
                        <!-- 19 -->
                        <th data-i18n="device.management.action">Action</th>
                        <!-- 19 -->
                        <!--<th data-i18n="device.management.min_quantity">Default Min Quantity</th>-->
                        <!-- 19 -->
                        <!--<th>GAP</th>-->
                        <!-- 19 -->
                        <!--<th>Risk</th>-->
                        <!-- 19
                        <th data-i18n="device.management.life_cycle">Life Cycle</th>-->
                    </tr>
                </thead>
                <tbody id="table_Devices_tbody">
                </tbody>
            </table>
        </div>
    </div>
</div>

<!-- Edit modal -->
<div class="modal fade" id="device_edit-modal" aria-hidden="true">
    <div class="modal-dialog modal-fullscreen modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="device_edit-title" data-i18n="device.management.update_device">Update Device</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!-- Details -->
                <h5 class="font-weight-light text-center text-primary py-3" id="borrow_modal-name" data-i18n="device.management.device_details" style="text-transform:uppercase">DEVICE DETAILS</h5>

                <!-- Basic Info -->
                <div class="row">
                    <div class="col-8">
                        <div class="card">
                            <div class="card-body">
                                <h6 class="mb-0 text-uppercase" data-i18n="device.management.basic_info">Basic Information</h6>
                                <hr>

                                <div class="row">
                                    <div class="col-6">
                                        <!-- Id + Device Code-->
                                        <div class="row">
                                            <div class="col-6">
                                                <div class="input-group mb-3">
                                                    <span class="col-6 input-group-text">ID</span>
                                                    <input class="form-control" type="text" id="device_edit-DeviceId" disabled>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="input-group mb-3" title="Mã thiết bị">
                                                    <span class="col-6 input-group-text" data-i18n="device.management.pn">PN</span>
                                                    <input class="form-control" type="text" id="device_edit-DeviceCode">
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Device Name -->
                                        <div class="input-group mb-3">
                                            <span class="col-4 input-group-text" data-i18n="device.management.description">Description</span>
                                            <input class="form-control" type="text" id="device_edit-DeviceName">
                                        </div>
                                        <!-- Specification -->
                                        <div class="input-group mb-3">
                                            <span class="col-4 input-group-text" data-i18n="device.management.specification">Specification</span>
                                            <input class="form-control" type="text" id="device_edit-Specification">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <!-- Status + Type -->
                                        <div class="row">
                                            <div class="col-6">
                                                <div class="input-group mb-3">
                                                    <span class="col-6 input-group-text" data-i18n="device.management.status">Status</span>
                                                    <select class="form-select" id="device_edit-Status">
                                                        <option value="Unconfirmed">Unconfirmed</option>
                                                        <option value="Part Confirmed">Part Confirmed</option>
                                                        <option value="Confirmed">Confirmed</option>
                                                        <option value="Locked">Locked</option>
                                                        <option value="Out Range">Out Range</option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="input-group mb-3">
                                                    <span class="col-6 input-group-text" data-i18n="device.management.type">Type</span>
                                                    <select class="col-6 form-select" id="device_edit-Type">
                                                        <option value="#N/A">N/A</option>
                                                        <option value="S">Static</option>
                                                        <option value="D">Dynamic</option>
                                                        <option value="Consign">Consign</option>
                                                        <option value="Fixture">Fixture</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Device Date -->
                                        <div class="input-group mb-3">
                                            <span class="col-4 input-group-text" data-i18n="device.management.created_date">Created date</span>
                                            <input class="form-control" type="datetime-local" id="device_edit-DeviceDate">
                                        </div>
                                        <!-- DeliveryTime -->
                                        <div class="input-group mb-3">
                                            <span class="col-4 input-group-text" data-i18n="device.management.lead_time">Lead Time</span>
                                            <input class="form-control" type="number" id="device_edit-DeliveryTime1">
                                            <select class="form-select" id="device_edit-DeliveryTime2">
                                                <option value="Day (Ngày)">Day (Ngày)</option>
                                                <option value="Week (Tuần)" selected>Week (Tuần)</option>
                                                <option value="Month (Tháng)">Month (Tháng)</option>
                                                <option value="Quarter (Quý)">Quarter (Quý)</option>
                                                <option value="Year (Năm)">Year (Năm)</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-4">
                        <div class="card">
                            <div class="card-body">
                                <div class="d-flex justify-content-between">
                                    <h6 class="mb-0 text-uppercase" data-i18n="device.management.image">Images</h6>
                                    <h6 class="text-primary cursor-pointer" id="add-image" data-i18n="device.management.add_image"> Add Image</h6>
                                </div>

                                <hr>
                                <div id="images-edit-container">
                                    <div class="carousel slide" data-ride="carousel">
                                        <ol class="carousel-indicators"></ol>
                                        <div class="carousel-inner"></div>
                                        <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
                                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
                                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <!-- Details Info-->
                <div class="card">
                    <div class="card-body">
                        <h6 class="mb-0 text-uppercase" data-i18n="device.management.details_info">Details Information</h6>
                        <hr>

                        <div class="row">
                            <div class="col-6">
                                <!-- WareHouse + Relation -->
                                <div class="row">
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.warehouse">Warehouse</span>
                                            <select class="form-select" id="device_edit-WareHouse"></select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        @*<div class="input-group mb-3" title="Mối liên hệ">
                                                <span class="col-5 input-group-text">Relation</span>
                                                <input class="form-control" type="text" id="device_edit-Relation">
                                            </div>*@
                                    </div>
                                </div>

                                <!-- Buffer + LifeCycle-->
                                <div class="row">
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.buffer">Buffer</span>
                                            <input class="form-control" type="number" id="device_edit-Buffer">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.life_cycle">Life Cycle</span>
                                            <input class="form-control" type="number" id="device_edit-LifeCycle">
                                        </div>
                                    </div>
                                </div>

                                <!-- BOM Quantity  + PO Quantity -->
                                <div class="row">
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.bom_qty">BOM Quantity</span>
                                            <input class="form-control" type="number" id="device_edit-Quantity">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.po_qty">PO Quantity</span>
                                            <input class="form-control" type="number" id="device_edit-POQty">
                                        </div>
                                    </div>
                                </div>

                                <!-- QtyConfirm + RealQty -->
                                <div class="row">
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.confirm_qty">Confirm Quantity</span>
                                            <input class="form-control" type="number" id="device_edit-QtyConfirm">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text fw-bold text-info" data-i18n="device.management.real_qty">Real Quantity</span>
                                            <input class="form-control" type="number" id="device_edit-RealQty">
                                        </div>
                                    </div>
                                </div>

                                <!-- Min Quantity + Unit -->
                                <div class="row">
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.min_quantity">Min Quantity</span>
                                            <input class="form-control" type="number" id="device_edit-MinQty">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="input-group mb-3">
                                            <span class="col-5 input-group-text" data-i18n="device.management.unit">Unit</span>
                                            <input class="form-control" type="text" id="device_edit-Unit" list="device_edit-Unit_List" maxlength="50">
                                            <datalist id="device_edit-Unit_List">
                                                <option value="N/A">N/A</option>
                                                <option value="PCS">PCS (Cái)</option>
                                                <option value="BAG">BAG (Bao)</option>
                                                <option value="BOX">BOX (Hộp)</option>
                                                <option value="SET">SET (Bộ)</option>
                                                <option value="M">M (Meter)</option>
                                                <option value="CM">CM (Centimet)</option>
                                                <option value="KG">KG (Kilogram)</option>
                                                <option value="G">G (Gram)</option>
                                                <option value="L">L (Liter)</option>
                                                <option value="ML">ML (Mililiter)</option>
                                            </datalist>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-6">
                                <!-- Product -->
                                <div class="input-group mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.product">Product</span>
                                    <input class="form-control" type="text" id="device_edit-Product" list="device_edit-Product-List" />
                                    <datalist id="device_edit-Product-List"></datalist>
                                    @*<select class="col-9 form-select" id="device_edit-Product"></select>*@
                                </div>
                                <!-- Model -->
                                <div class="input-group mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.model">Model</span>
                                    <input class="form-control" type="text" id="device_edit-Model" list="device_edit-Model-List" />
                                    <datalist id="device_edit-Model-List"></datalist>
                                    @*<select class="col-9 form-select" id="device_edit-Model"></select>*@
                                </div>
                                <!-- Station -->
                                <div class="input-group mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.station">Station</span>
                                    <input class="form-control" type="text" id="device_edit-Station" list="device_edit-Station-List" />
                                    <datalist id="device_edit-Station-List"></datalist>
                                    @*<select class="col-9 form-select" id="device_edit-Station"></select>*@
                                </div>
                                <!-- Group -->
                                <div class="input-group mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.group">Group</span>
                                    <input class="form-control" type="text" id="device_edit-Group" list="device_edit-Group-List" />
                                    <datalist id="device_edit-Group-List"></datalist>
                                    @*<select class="col-6 form-select" id="device_edit-Group"></select>*@
                                </div>
                                <!-- Vendor -->
                                <div class="input-group mb-3">
                                    <span class="col-3 input-group-text" data-i18n="device.management.vendor">Vendor</span>
                                    <input class="form-control" type="text" id="device_edit-Vendor" list="device_edit-Vendor-List" />
                                    <datalist id="device_edit-Vendor-List"></datalist>
                                    @*<select class="col-6 form-select" id="device_edit-Vendor"></select>*@
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Layout -->
                <h5 class="font-weight-light text-center text-primary py-3" id="borrow_modal-name" data-i18n="device.management.device_location">DEVICE LOCATION</h5>
                <div id="layout-container"></div>
                <button type="button" class="btn btn-sm btn-outline-secondary w-100" id="new-layout" data-i18n="device.management.add_layout"><i class="fa-solid fa-plus"></i> Add Layout</button>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" data-i18n="global.close">Close</button>
                <button type="button" class="btn btn-primary" id="button-save_modal" data-i18n="global.save">Save</button>
            </div>
        </div>
    </div>
</div>

<!-- Details modal -->
@Html.Partial("~/Areas/NVIDIA/Views/DeviceManagement/_DeviceDetails_Modal.cshtml")

<!-- Borrow Request modal -->
@Html.Partial("~/Areas/NVIDIA/Views/BorrowManagement/_RequestDetails_Modal.cshtml")

@section scripts{
    <!-- Vendor -->
    <script src="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/datatable/js/dataTables.bootstrap5.min.js"></script>
    <script src="~/Areas/NVIDIA/Content/Vendor/MomentJs/moment.js"></script>
    <script src="~/Areas/NVIDIA/Content/Template/Sysadmin/assets/plugins/select2/js/select2.min.js"></script>
    <script src="~/Areas/NVIDIA/Content/Vendor/Viewer JS/viewer.js"></script>
    <!-- Custom -->
    <script src="~/Areas/NVIDIA/Content/Custom/DeviceManagement/js/DeviceManagement.js?v=@Guid.NewGuid()"></script>
    <script src="~/Areas/NVIDIA/Content/Custom/DeviceManagement/js/DeviceDetails.js?v=@Guid.NewGuid()"></script>
    <script src="~/Areas/NVIDIA/Content/Custom/DeviceManagement/js/DeviceUpdate.js?v=@Guid.NewGuid()"></script>
    <script src="~/Areas/NVIDIA/Content/Custom/BorrowManagement/js/RequestDetails.js?v=@Guid.NewGuid()"></script>
    <script type="text/javascript">
        // Warehouse
        $('#input_WareHouse', document).select2({
            theme: 'bootstrap4',
            minimumResultsForSearch: -1,
        });

        // Modal
        $('#device_edit-AccKit').select2({
            theme: 'bootstrap4',
            dropdownParent: $("#device_edit-modal"),
            minimumResultsForSearch: -1,
            width: '50%',
        });
        $('#device_edit-Type').select2({
            theme: 'bootstrap4',
            dropdownParent: $("#device_edit-modal"),
            minimumResultsForSearch: -1,
            width: '50%',
        });
        $('#device_edit-Status').select2({
            theme: 'bootstrap4',
            dropdownParent: $("#device_edit-modal"),
            minimumResultsForSearch: -1,
            width: '50%',
        });
        $('#device_edit-WareHouse').select2({
            theme: 'bootstrap4',
            dropdownParent: $("#device_edit-modal"),
            minimumResultsForSearch: -1,
            width: '50%',
        });

        // Filter
        $(document).ready(function () {
            var dropdownOpen = false;
            $('#dropdown_filter').on('click', function (event) {
                event.stopPropagation();

                dropdownOpen = !dropdownOpen;

                if (dropdownOpen) {
                    $(this).addClass('show');
                } else {
                    $(this).removeClass('show');
                }
            });

            $(document).on('click', function (event) {
                if (dropdownOpen && !$(event.target).closest('#dropdown_filter').length) {
                    $('#dropdown_filter').removeClass('show');
                    dropdownOpen = false;
                }
            });
        });
